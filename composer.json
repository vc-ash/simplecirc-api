{
    "name": "vc-ash/simplecirc-api",
    "version": "1.0",
    "type": "library",
    "description": "A PHP library for working with the SimpleCirc API.",
    "keywords": ["magazine","simplecirc", "SimpleCirc"],
    "homepage": "https://github.com/vc-ash",
    "license": "MIT",
    "authors": [
        {
            "name": "Ash Bulcroft",
            "email": "ashbulcroft@gmail.com",
            "homepage": "https://github.com/vc-ash",
            "role": "Lead"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vc-ash/helper-functions"
        }
    ],
    "require": {
        "php": ">=5.6",
        "vc-ash/helper-functions": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5"
    },
    "autoload": {
        "psr-4": {
            "SimpleCircApi\\": "src/"
        },
        "files": [
            "../helper-functions/functions/build_url.php",
            "../helper-functions/functions/concat_path.php",
            "../helper-functions/functions/pre_print.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleCircApi\\Tests\\": "tests/"
        }
    }
}